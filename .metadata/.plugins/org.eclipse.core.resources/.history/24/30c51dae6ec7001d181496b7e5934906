package P5;

import java.util.Scanner;

public class arregloint {

	
	public static int[] getA(int [] a) {
		int min = 99999;
		int max = -9999;
		int prom = 0; 
		for (int num: a){
			if (num < min) {
				min = num;
			}
			if (num > max) {
				max = num;
			}
			prom += num;
		}
		prom = prom / a.length;
		int [] b = new int [3];
		b[0]= prom;
		b[1]= min;
		b[2]= max;
		
		return b;
		
		
	}
	
	public static void getB (Parametros P, int [] array) {
		int min = 99999;
		int max = -9999;
		int prom = 0; 
		for (int num: array){
			if (num < min) {
				min = num;
			}
			if (num > max) {
				max = num;
			}
			prom += num;
		}
		prom = prom / array.length;		
		P = new Parametros(3);
		P.agregar(0, prom);
		P.agregar(1, min);
		P.agregar(2, max);
		
	}
	
	public static void main(String[] args) {	
		int [] a1 =new int [10];
		Parametros P = new Parametros();
		
		
		for (int i=0;i<10;i++) {
		Scanner c = new Scanner(System.in);
		a1[i] = c.nextInt();
	}
	System.out.println(P.toString());
}
}
